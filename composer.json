{
  "name": "your-vendor/video-management",
  "description": "Video Management System with AWS S3 Integration",
  "type": "project",
  "require": {
    "php": ">=7.4",
    "aws/aws-sdk-php": "^3.336",
    "vlucas/phpdotenv": "^5.5",
    "monolog/monolog": "^2.9",
    "phpmailer/phpmailer": "^6.8",
    "intervention/image": "^2.7",
    "guzzlehttp/guzzle": "^7.0",
    "firebase/php-jwt": "^6.0",
    "respect/validation": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "symfony/var-dumper": "^5.4",
    "filp/whoops": "^2.15",
    "fakerphp/faker": "^1.23"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable"
}
